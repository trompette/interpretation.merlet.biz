{
    "name": "trompette/interpretation.merlet.biz",
    "description": "my wife's website",
    "require": {
        "php": "^7.3",
        "symfony/console": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/flex": "^1.13",
        "symfony/framework-bundle": "^5.3",
        "symfony/runtime": "^5.3",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/twig-bundle": "^5.3",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.3",
        "symfony/css-selector": "^5.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sveta\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0EM3TK8H6VN1CDDPA63Z0VM",
            "allow-contrib": false,
            "require": "^5.3"
        }
    }
}
