{
    "name": "trompette/interpretation.merlet.biz",
    "description": "my wife's website",
    "require": {
        "php": "^7.3",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^1.19",
        "symfony/framework-bundle": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/runtime": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4"
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "Sveta\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0EM3TK8H6VN1CDDPA63Z0VM",
            "allow-contrib": false,
            "require": "^5.4",
            "docker": false
        }
    }
}
