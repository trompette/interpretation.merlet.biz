{
    "name": "trompette/interpretation.merlet.biz",
    "description": "my wife's website",
    "require": {
        "php": "^7.3",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.4",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/twig-bundle": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/phpunit-bridge": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sveta\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0EM3TK8H6VN1CDDPA63Z0VM",
            "allow-contrib": false,
            "require": "^4.4"
        }
    }
}
